#include<bits/stdc++.h>
using namespace std;

int main(){
    vector<int> arr ={2,3,-4,3};
     int sum,maxx=0;
    
    for(int i=0;i<arr.size();i++){
        sum=1;
        for(int j=i;j<arr.size();j++){
             sum*=arr[j];
             if(maxx<sum){
                 maxx=sum;
             }
        }
      
       
    }
    
   
    cout<<" maxximum pruduct :" <<maxx;
   
}